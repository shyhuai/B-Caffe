net: "googlenet.prototxt"
test_iter: 1000
test_interval: 4000
test_initialization: false
#display: 1
display: 40
#average_loss: 40
#base_lr: 0.01
base_lr: 0.04 # for 4 nodes
lr_policy: "step"
stepsize: 320000
gamma: 0.96
#max_iter: 100
max_iter: 2400000
momentum: 0.9
weight_decay: 0.0002
snapshot: 40000
snapshot_prefix: "models"
solver_mode: GPU
#gradients_merged: false 
gradients_merged: true 
